<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    <PreferenceCategory
        android:key=""
        android:title="@string/settings_category_options">

        <CheckBoxPreference
            android:defaultValue="true"
            android:key="show_launcher_icon"
            android:title="@string/settings_item_show_launcher"/>

        <ListPreference
            android:defaultValue="default"
            android:dialogTitle="@string/settings_item_choose_theme"
            android:entries="@array/themeArray"
            android:entryValues="@array/themeValues"
            android:key="theme"
            android:title="@string/settings_item_choose_theme" />

        <CheckBoxPreference
            android:defaultValue="true"
            android:key="global_participation"
            android:summary="@string/settings_item_global_stats_summary"
            android:title="@string/settings_item_global_stats_title"/>

        <CheckBoxPreference
            android:defaultValue="true"
            android:key="enable_service_block"
            android:summary="@string/settings_item_enable_service_block_summery"
            android:title="@string/settings_item_enable_service_block"/>

        <CheckBoxPreference
            android:defaultValue="false"
            android:key="force_english"
            android:summary="@string/settings_item_force_english_summary"
            android:title="@string/settings_item_force_english_title"/>

        <Preference
            android:defaultValue="0"
            android:editable="false"
            android:key="reset_defaults"
            android:summary="@string/settings_item_reset_defaults_title"
            android:title="@string/settings_item_reset_defaults_title"
            />

        <ListPreference
            android:defaultValue="default"
            android:dialogTitle="@string/settings_item_logging_level"
            android:entries="@array/loggingArray"
            android:entryValues="@array/loggingValues"
            android:key="logging_level"
            android:title="@string/settings_item_logging_level" />

    </PreferenceCategory>

